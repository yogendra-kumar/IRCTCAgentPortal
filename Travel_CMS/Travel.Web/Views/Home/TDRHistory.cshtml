	<div class="col-md-10">
          <div class="row">
            <div class="col-sm-12">	
			<div class="panel panel-primary mt5 ml5">
				<div class="panel-heading">
					<h5 style="align:center">List of Tickets</h5>
				</div>
				<table id="pexample1" class="table table-bordered table-striped table-responsive" role="grid" aria-describedby="pexample1_info"
					ng-model="TdrTabledata">
					<thead>
						<tr style="background-color: honeydew">
							<td >Select</td>
							<td class="hidden-sm hidden-xs">Ticket Number</td>
							<td>PNR Number</td>
							<td>TDR Reason</td>
							<td class="hidden-sm hidden-xs">Status</td>
							<td class="hidden-sm hidden-xs">TDR Date</td>
						</tr>
					</thead>
					<!--"From":"ndls","To":"bsb","BookedTime":"9/11/2016","Txnno":"343434341","Pnr":"22222222222211","journeydate":"23/11/2016","status":"zz"},-->

					<tbody>
						<tr ng-repeat="tdr in TdrTabledata">
							<td><span><input type="checkbox" name="cbSelect" ng-model="tdr.checked" value="{{ tdr }}" /> </span></td>
							<td class="hidden-sm hidden-xs"><span>{{ tdr.pnr}}</span></td>
							<td><span><a href="" class="btn-link" ng-click="OpenPopup(tdr)">{{tdr.pnr}}</a></span></td>
							<td><span>{{tdr.reason}} </span></td>
							<td class="hidden-sm hidden-xs"><span>{{ tdr.status }} </span></td>
							<td class="hidden-sm hidden-xs"><span>{{tdr.tdrSentDate}} </span></td>
						</tr>
					</tbody>

				</table>
			</div>
            </div>
          </div>
	</div>
	</form>
	

<div class="modal fade" id="TdrTicketModal" role="dialog">
			<div class="modal-dialog modal-lg">
				<div class="modal-content">
					<div class="modal-header">
						<button type="button" class="close" data-dismiss="modal">&times;</button>
						<h4 class="modal-title">{{Header}}</h4>
					</div>				
					
            <div class="modal-body">
						<form class="form-horizontal" novalidate name="TDRTicketForm">
							<div class="panel panel-primary">
								<div class="panel-heading">
									<h3 class="panel-title">Ticket Detail</h3>
								</div>
								<table class="table modelTable" ng-model="pnrData">
									<tr>
										<th>PNR Number</th>
										<td>{{pnrData.pnr}}</td>
										<th>TDR Filling Date </th>
										<td>{{pnrData.tdrSentDate}}</td>
									</tr>
									<tr>
										<th>Ticket Number</th>
										<td>{{pnrData.pnr}}</td>
										<th>Status </th>
										<td>{{pnrData.status}}</td>
									</tr>
									<tr>
										<th>Refund Amount</th>
										<td>{{pnrDta.amountRefunded}}<p ng-if="!pnrDta.amountRefunded">0.0</p></td>
										<th>Refund Date </th>
										<td>{{pnrData.refundedDate}}</td>
									</tr>
									<tr>
										<th>Reason</th>
										<td>{{pnrData.reason}}</td>
										<th>Confirmed</th>
										<td>{{pnrData.confirmed}}</td>
									</tr>
								</table>
							</div>
						</form>
            </div>
			<div class="modal-footer">
						<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
			</div>
				</div>
			</div>
		</div>
